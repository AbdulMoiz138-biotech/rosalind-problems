      "source": [
        "def rabbit_pairs(n,k):\n",
        "    if n==1 or n==2:\n",
        "      return 1\n",
        "    pervious=1 #f1\n",
        "    current=1  #f2\n",
        "    for i in range(3,n+1):\n",
        "      new = current + k * pervious\n",
        "      pervious, current = current, new\n",
        "    return current\n",
        "\n",
        "\n",
        "print(rabbit_pairs(35,3))\n",
        "\n"
      ]
    }
  ]
}